<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>%(ClientBuilder.plist_label)</string>
	<key>UserName</key>
	<string>root</string>
	<key>GroupName</key>
	<string>daemon</string>
	<key>ProgramArguments</key>
	<array>
		<string>%(ClientBuilder.plist_binary_path)</string>
		<string>--config=%(ClientBuilder.plist_binary_prefix)/%(ClientBuilder.output_basename)/%(PyInstaller.config_name)</string>
	</array>
	<key>KeepAlive</key>
	<true/>
	<key>RunAtLoad</key>
	<true/>
	<key>ServiceDescription</key>
	<string>%(Client.name)</string>
	<key>StandardOutPath</key>
	<string>/dev/null</string>
	<key>StandardErrorPath</key>
	<string>/dev/null</string>
</dict>
</plist>
