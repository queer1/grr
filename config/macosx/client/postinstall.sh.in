#!/bin/sh
#
# Post installation script for GRR client MacOS-X package

# Run the installation routines.
"%(ClientBuilder.plist_binary_path)" --install --config="%(ClientBuilder.plist_binary_prefix)/%(ClientBuilder.output_basename)/%(PyInstaller.config_name)"

if [ -f "%(ClientBuilder.plist_path)" ];
then
  sudo launchctl load "%(ClientBuilder.plist_path)";
fi

